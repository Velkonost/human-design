<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle" >
            <gradient
                android:angle="360"
                android:startColor="#4D8DBC"
                android:endColor="#5352BD"
                android:type="linear" />
            <corners android:radius="20dp"/>
            <!--            <size android:width="24dp"-->
            <!--                android:height="24dp"/>-->
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <solid android:color="#F8F8F8" />
            <corners android:radius="20dp"/>
        </shape>
    </item>

</layer-list>

